{
    "name": "themsaid/laravel-mail-preview",
    "description": "Adds a 'preview' mail driver for testing emails in laravel.",
    "keywords": ["laravel", "mail"],
    "homepage": "https://github.com/themsaid/laravel-preview-mail-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "illuminate/mail": "^6.0",
        "illuminate/filesystem": "^6.0"

    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\MailPreview\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\MailPreview\\MailPreviewServiceProvider"
            ]
        }
    }
}
